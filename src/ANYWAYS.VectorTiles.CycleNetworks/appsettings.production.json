{
  "webSource": "http://planet.anyways.eu/planet/extracts/belgium-and-neighbourhood-latest.osm.pbf",
  "source": "/var/app/source/data.osm.pbf",
  "target": "/var/app/target/",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console"
      }
    ]
  }
}